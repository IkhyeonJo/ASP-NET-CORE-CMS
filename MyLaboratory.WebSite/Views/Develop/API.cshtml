@{ 
    string webAPIUrl = $"{MyLaboratory.WebSite.Common.ServerSetting.DomainName.Remove(MyLaboratory.WebSite.Common.ServerSetting.DomainName.Length - 1)}:5001";
}
<section class="content">
    <div class="card card-primary">
        <div class="card-header">
            <h3 class="card-title">@localizer["Develop API"]</h3>
        </div>
        <div class="card-body">
            <a href='@webAPIUrl' target='_blank' rel='noopener noreferrer'>MyLaboratory.WebAPI</a>
        </div>
    </div>
</section>